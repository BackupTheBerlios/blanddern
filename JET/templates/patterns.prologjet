<%@ jet package="patterns" imports="adaptor.* org.eclipse.emf.ecore.*" class="PatternsGeneration"%>
<% Matching m = (Matching) argument; %>

<% for(int i=0; i<m.getLhs().getComposed().size(); i++){ %>
	<% EClass concept = m.getLhs().getComposed().get(i).getConcept(); %>
	
	create(Node,N1,<%=concept.getName().toLowerCase()%>,N2).
	addProperty(<%=concept.getName().toLowerCase()%>,N1,name,Value,N2).	
<% } %>


		